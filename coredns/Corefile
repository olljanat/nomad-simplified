  errors
  health :8080 {
    lameduck 1s
  }
  rewrite stop type AAAA A
  log
  cache
  nomad service.nomad {
    address http://127.0.0.1:4646
    filter "Tags not contains candidate"
    ttl 10
  }
  loadbalance {
    prefer {$COREDNS_PREFER_CIDR}
  }
}

.:53 {
  cache
  errors
  forward . 127.0.0.11
}
